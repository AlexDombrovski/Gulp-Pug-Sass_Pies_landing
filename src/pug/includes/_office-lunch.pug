//- стили к этому блоку в _pies.sass

.container-fluid.pies-wrapper
  .container.pies-cont
    h2.pies-title(class="wow fadeInLeftBig" data-wow-delay="0.1s") #{officeLunchTitle}
    .pies-cards
      each item in officeLunch
        +item-card(item.img, item.name, item.structure, item.price, item.weight)